{
    "name": "ryna-language-support",
    "displayName": "Ryna Language Support",
    "description": "Basic support for the Ryna Programming Language",
    "publisher": "RynaLanguage",
    "icon": "img/icon.png",
    "repository": {
        "url": "https://github.com/Gabie-of-the-Bo/Ryna-lang-support"
    },
    "version": "0.1.6",
    "engines": {
        "vscode": "^1.62.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "ryna",
            "aliases": ["Ryna", "ryna"],
            "extensions": [".ryna"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "ryna",
            "scopeName": "source.ryna",
            "path": "./syntaxes/ryna.tmLanguage.json"
        }]
    }
}